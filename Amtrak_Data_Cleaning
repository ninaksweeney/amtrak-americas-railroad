   "source": [
    "def clean_name(row):\n",
    "    if 'Micro Area' in row:\n",
    "        new_name = row.replace(' Micro Area', \"\")\n",
    "    elif 'Metro Area' in row: \n",
    "        new_name = row.replace(' Metro Area', \"\")\n",
    "    else: \n",
    "        new_name=row\n",
    "    return new_name\n",
    "\n"
   ]
