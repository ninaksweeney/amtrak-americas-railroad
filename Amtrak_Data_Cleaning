  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "b0066a08",
   "metadata": {},
   "outputs": [],
   "source": [
    "for col in ['total_pop_estimate','15_to_19','20_to_24','25_to_29','30_to_34']:\n",
    "    census_age[col] = census_age[col].astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "9f9464a5",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_pop_percentage(pop_estimate, subgroup):\n",
    "    percentage = subgroup/pop_estimate\n",
    "    return percentage*100\n",
    "\n",
    "for col in ['15_to_19', '20_to_24', '25_to_29', '30_to_34']:\n",
    "    census_age[f'{col}_percentage'] = census_age.apply(lambda x: get_pop_percentage(x.total_pop_estimate, x[col]), axis=1)\n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "dfdcfd72",
   "metadata": {},
   "outputs": [],
   "source": [
    "census_age['total_young_pop'] = census_age['15_to_19'] + census_age['20_to_24']+census_age['25_to_29']+census_age['30_to_34']\n",
    "census_age['young_pop_percent'] = (census_age.total_young_pop / census_age.total_pop_estimate) *100"
   ]
