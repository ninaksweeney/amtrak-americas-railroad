   "source": [
    "def clean_CBSA(row):\n",
    "    if row == \"Raleigh-Cary, NC\":\n",
    "        return \"Raleigh, NC\"\n",
    "    elif row == \"Sacramento-Roseville-Folsom, CA\":\n",
    "        return \"Sacramento--Roseville--Arden-Arcade, CA\"\n",
    "    else:\n",
    "        return row\n",
    "    \n",
    "school_tuition_latlong['NMCBSA'] = school_tuition_latlong['NMCBSA'].apply(clean_CBSA)"
   ]
